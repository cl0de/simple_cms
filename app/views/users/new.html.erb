<% provide(:title, "Sign up")  %>
    
    <h1>Sign Up</h1>
    <div class="row">
        <div class="col-6 col-offset-3">
            <%= form_for(@user) do |f| %>
            <%= render 'shared/error_messages' %>
            <%= render partial: "form", :locals => {:f => f} %>
            <%= f.submit "Create my account", class: "btn btn-primary" %>
            <% end %>
        </div>
    </div>
<p>This will be the page for the new user</p>
